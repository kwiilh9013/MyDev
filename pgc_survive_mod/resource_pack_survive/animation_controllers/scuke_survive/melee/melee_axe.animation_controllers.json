                    ],
                    "transitions": [
                        { "idle": "query.all_animations_finished && !query.mod.scuke_survive_melee_using" },
                        { "melee_attack3": "query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 3" }
                    ]
                },
                "melee_attack3": {
                    "animations": [
                        "scuke_survive_melee_axe_first_melee_attack3"
                    ],
                    "transitions": [
                        { "idle": "query.all_animations_finished && !query.mod.scuke_survive_melee_using" },
                        { "melee_attack1": "query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 1" }
                    ]
                }
            }
        },
        "controller.animation.scuke_survive_melee_axe_third" : {
