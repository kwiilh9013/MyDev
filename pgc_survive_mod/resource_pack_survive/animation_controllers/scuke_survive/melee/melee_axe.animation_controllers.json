{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.scuke_survive_melee_axe_first": {
            "initial_state": "idle",
            "states": {
                "idle": {
                    "animations": [
                        "scuke_survive_melee_axe_first_hold"
                    ],
                    "transitions" : [
                        { "melee_begin": "query.mod.scuke_survive_melee_begin" },
                        { "melee_attack1": "query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 1" },
                        { "melee_attack2": "query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 2" },
                        { "melee_attack3": "query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 3" }
                    ]
                },
                "melee_begin": {
                    "animations": [
                        "scuke_survive_melee_axe_first_melee_begin"
                    ],
                    "transitions": [
                        { "idle": "query.all_animations_finished && !query.mod.scuke_survive_melee_begin" }
                    ]
                },
                "melee_attack1": {
                    "animations": [
                        "scuke_survive_melee_axe_first_melee_attack1"
                    ],
                    "transitions": [
