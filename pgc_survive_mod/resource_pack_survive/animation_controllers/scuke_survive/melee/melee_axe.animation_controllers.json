                        { "melee_begin" : "!query.is_in_ui && query.mod.scuke_survive_melee_begin" },
                        { "melee_attack1": "!query.is_in_ui && query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 1" },
                        { "melee_attack2": "!query.is_in_ui && query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 2" },
                        { "melee_attack3": "!query.is_in_ui && query.mod.scuke_survive_melee_using && query.mod.scuke_survive_melee_attack == 3" }
                    ]
                },
                "melee_begin": {
