{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "scuke_survive:npc_wheelchair_man",
            "materials": {
                "default": "villager_v2"
            },
            "textures": {
                "default": "textures/entity/scuke_survive/npc/wheelchair_man"
            },
            "geometry": {
                "default": "geometry.scuke_survive_npc_wheelchair_man"
            },
            "render_controllers": [
                "controller.render.default"
            ],
            "spawn_egg": {
                "base_color":"#8B9CA1",
                "overlay_color":"#455057"
            },
            "animations": {
                "look_at_target": "animation.common.look_at_target",
                "walk": "animation.scuke_survive.wheelchair_man.walk",
                "ui_correct": "animation.scuke_survive.wheelchair_man.ui_correct"
            },
            "scripts": {
                "pre_animation": [
                    "variable.num_professions = 15;",
                    "variable.num_tiers = 3;",
                    "variable.profession_index = (query.variant < variable.num_professions ? query.variant : 0);",
                    "variable.level_index = query.trade_tier;",
                    "variable.not_in_ui = 1.0;",
                    "variable.correct_pos = 0.0;"
                ],
                "animate":[
                    {
                        "walk" : "query.modified_move_speed"
                    },
                    {
                        "look_at_target": "variable.not_in_ui"
                    },
                    {
                        "ui_correct": "variable.correct_pos == 1.0"
                    },
                    {
                        "walk": "variable.correct_pos == 1.0"
                    }
                ]
            }
        }
    }
}