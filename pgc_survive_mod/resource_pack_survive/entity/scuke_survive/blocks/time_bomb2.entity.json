                "time_flowing": "animation.scuke_time_bomb.time_flowing",
                "place_anim": "animation.scuke_time_bomb.place_anim",
                "left_time": "animation.scuke_time_bomb.left_time"
            },
            "scripts": {
                "should_update_bones_and_effects_offscreen": true,
                "initialize": [
                    "variable.block_type = 1.0;",
                    "variable.total_time = 0.0;",
                    "variable.left_time = 0.0;",
                    "variable.has_set_time = 0.0;"
                ],
                "pre_animation": [
                    "variable.correct_pos = 0.0;",

