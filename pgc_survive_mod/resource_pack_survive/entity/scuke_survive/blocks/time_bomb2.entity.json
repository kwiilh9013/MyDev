{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "identifier": "scuke_survive:time_bomb2_entity",
            "materials": {
                "default": "scuke_survive_entity_light",
                "blend": "scuke_survive_npc_golden_creeper_outer"
            },
            "textures": {
                "default": "textures/entity/scuke_survive/blocks/time_bomb"
            },
