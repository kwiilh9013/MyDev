{
    "format_version": "1.10.0",
    "minecraft:client_entity": {
        "description": {
            "animations": {
                "move": "animation.arrow.move",
                "idle": "animation.scuke_zombie_pjt.idle",
                "rotate": "animation.scuke_zombie_pjt.rotate",
                "ctrl": "controller.animation.projectile_random_zombie.switch"
            },
            "enable_attachables": true,
            "geometry": {
                "default": "geometry.scuke_zombie_normal"
            },
            "identifier": "scuke_survive:projectile_random_zombie",
            "materials": {
                "default": "scuke_survive_zombie"
            },
            "render_controllers": [
                "controller.render.default"
            ],
            "scripts": {
                "pre_animation": [
                 "variable.shake = query.shake_time - query.frame_alpha;",
                 "variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
                ],
                "animate": [
                    "ctrl"
                ]
            },
            "textures": {
                "default": "textures/entity/scuke_survive/zombie/scuke_zombie_normal1"
            }
        }
    }
}