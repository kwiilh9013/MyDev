						"ctl_attack": "variable.not_in_ui"
					},
					{
						"ctl_hurt_effect": "variable.not_in_ui"
					},
                    {
                        "ui_correct": "variable.correct_pos == 1.0"
                    },
                    {
                        "standby": "variable.correct_pos == 1.0"
                    }
				]
			},
			"textures": {
				"default": "textures/entity/scuke_survive/zombie/scuke_zombie_jocker_king"
