{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"animations": {
				"ui_correct": "animation.scuke_zombie_ui.grandpa_correct",
				"move_paper_disappear": "animation.scuke_zombie_grandpa.move_paper_disappear",
				"standby_paper_disappear": "animation.scuke_zombie_grandpa.standby_paper_disappear",
				"attack_paper_disappear": "animation.scuke_zombie_grandpa.attack_paper_disappear",
				"move": "animation.scuke_zombie_grandpa.move",
				"standby": "animation.scuke_zombie_grandpa.standby",
				"hunting": "animation.scuke_zombie.hunting",
				"attack": "animation.scuke_zombie_grandpa.attack",
				"death": "animation.scuke_zombie.death",
				"ctl_scuke_zombie_grandpa":"controller.animation.scuke_zombie_grandpa",
				"look_at_target_default": "animation.humanoid.look_at_target.default",
                "look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
                "look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
				"ctl_look_at_target": "controller.animation.humanoid.look_at_target"
			},
			"geometry": {
				"default": "geometry.scuke_zombie_grandpa"
			},
			"spawn_egg": {
				"base_color":"#695d54",
                "overlay_color":"#cec7b5"
			},
			"identifier": "scuke_survive:zombie_grandpa",
			"materials": {
				"default": "scuke_survive_zombie"
			},
			"render_controllers": [
				"controller.render.scuke_zombie"
			],
			"scripts": {
				"pre_animation": [
					"variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
					"variable.hurt_effect = (query.time_stamp - query.mod.hurt_time) < query.mod.hurt_duration;",
					"variable.is_moving = query.ground_speed >= 0.01;",
                    "variable.not_in_ui = 1.0;",
                    "variable.correct_pos = 0.0;"
				],
				"animate": [
					{
						"ctl_scuke_zombie_grandpa": "variable.not_in_ui"
					},
					{
						"ctl_look_at_target": "variable.not_in_ui"
					},
                    {
                        "ui_correct": "variable.correct_pos == 1.0"
                    },
                    {
                        "attack": "variable.correct_pos == 1.0"
                    }
				]
			},
			"textures": {
				"default": "textures/entity/scuke_survive/zombie/scuke_zombie_grandpa"
			}
		}
	}
}