{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"animations": {
				"look_at_target": "animation.humanoid.look_at_target.default",
				"idle": "animation.scuke_zombie.idle3",
				"move": "animation.scuke_zombie.walk",
				"hunting": "animation.scuke_zombie.hunting",
				"attack": "animation.scuke_zombie.attack",
				"death": "animation.scuke_zombie.death",
				"crawl": "animation.scuke_zombie_gangs.crawl",
				"crawl_notmove": "animation.scuke_zombie_gangs.crawl_notmove",
				"crawl_attack":"animation.scuke_zombie.crawl_attack",
				"ctl_scuke_zombie_cancrawl": "controller.animation.scuke_zombie_cancrawl.normal",
				"hurt_effect": "animation.scuke_zombie.hurt_effect",
				"ctl_hurt_effect": "controller.animation.scuke_zombie_hurt_effect",
				"ui_correct": "animation.scuke_zombie_ui.gangs_correct"
			},
			"geometry": {
				"default": "geometry.scuke_zombie_normal"
			},
			"spawn_egg": {
				"base_color": "#4e4741",
				"overlay_color": "#292a44"
			},
			"identifier": "scuke_survive:zombie_gangs",
			"materials": {
				"default": "scuke_survive_zombie"
			},
			"render_controllers": [
				"controller.render.scuke_zombie_gangs"
			],
			"scripts": {
				"pre_animation": [
					"variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
					"variable.hurt_effect = (query.time_stamp - query.mod.hurt_time) < query.mod.hurt_duration;",
					"variable.is_moving = query.ground_speed >= 0.01;",
					"variable.mutilated_blood = query.health / query.max_health <= 0.2;",
                    "variable.not_in_ui = 1.0;",
                    "variable.correct_pos = 0.0;"
				],
				"animate": [
					{
						"look_at_target": "!variable.mutilated_blood && query.is_alive"
					},
					{
						"ctl_scuke_zombie_cancrawl": "variable.not_in_ui"
					},
					{
						"ctl_hurt_effect": "variable.not_in_ui"
					},
                    {
                        "ui_correct": "variable.correct_pos == 1.0"
                    },
                    {
                        "move": "variable.correct_pos == 1.0"
                    }
				]
			},
			"textures": {
				"variant0": "textures/entity/scuke_survive/zombie/scuke_zombie_gangs1",
				"variant1": "textures/entity/scuke_survive/zombie/scuke_zombie_gangs2"
			}
		}
	}
}