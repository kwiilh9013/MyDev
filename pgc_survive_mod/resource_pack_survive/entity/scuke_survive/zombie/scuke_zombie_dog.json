{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"animations": {
				"look_at_target_default": "animation.humanoid.look_at_target.default",
                "look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
                "look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
				"ctl_scuke_zombie_dog": "controller.animation.scuke_zombie_dog",
				"standby": "animation.scuke_zombie_dog.standby",
				"move": "animation.scuke_zombie_dog.move",
				"attack3": "animation.scuke_zombie_dog.attack3",
				"ui_correct": "animation.scuke_zombie_ui.dog_correct",
				"ctl_look_at_target": "controller.animation.humanoid.look_at_target"
			},
			"geometry": {
				"default": "geometry.scuke_zombie_dog"
			},
			"spawn_egg": {
				"base_color": "#212632",
				"overlay_color": "#ff0000"
			},
			"identifier": "scuke_survive:zombie_dog",
			"materials": {
				"default": "scuke_survive_zombie"
			},
			"render_controllers": [
				"controller.render.scuke_zombie_dog"
			],
			"scripts": {
				"initialize": [
					"variable.is_moving = 0;"
				],
				"pre_animation": [
					"variable.hurt_effect = (query.time_stamp - query.mod.hurt_time) < query.mod.hurt_duration;",
					"variable.is_moving = query.ground_speed >= 0.01;",
                    "variable.not_in_ui = 1.0;",
                    "variable.correct_pos = 0.0;"
				],
				"animate": [
					{
						"ctl_look_at_target": "variable.not_in_ui"
					},
					{
						"ctl_scuke_zombie_dog": "variable.not_in_ui"
					},
                    {
                        "ui_correct": "variable.correct_pos == 1.0"
                    },
                    {
                        "attack3": "variable.correct_pos == 1.0"
                    }
				]
			},
			"textures": {
				"zombie_dog_default": "textures/entity/scuke_survive/zombie/scuke_zombie_dog1",
				"zombie_dog_police": "textures/entity/scuke_survive/zombie/scuke_zombie_dog2"
			}
		}
	}
}