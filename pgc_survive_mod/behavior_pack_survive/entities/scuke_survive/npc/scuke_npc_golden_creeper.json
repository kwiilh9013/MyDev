{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "scuke_survive:npc_golden_creeper",
            "is_spawnable": true,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "minecraft:exploding": {
                "minecraft:explode": {
                    "fuse_length": 1.5,
                    "fuse_lit": true,
                    "power": 4,
                    "causes_fire": true,
                    "destroy_affected_by_griefing": true
                },
                "minecraft:is_shaking": {}
            },
            "minecraft:forced_exploding": {
                "minecraft:target_nearby_sensor": {},
                "minecraft:explode": {
                    "fuse_length": 1.5,
                    "fuse_lit": true,
                    "power": 3,
                    "causes_fire": false,
                    "destroy_affected_by_griefing": true
                },
                "minecraft:on_target_escape": {},
                "minecraft:is_shaking": {}
            },
            "minecraft:creeper_tamed": {
                "minecraft:type_family": {
                    "family": [
                        "mercenary",
                        "npc"
                    ]
                },
                "minecraft:is_tamed": {},
                "minecraft:sittable": {},
                "minecraft:behavior.tempt": {
                    "priority": 5,
                    "speed_multiplier": 1.2,
                    "within_radius": 16,
                    "items": [
                        "minecraft:tnt"
                    ]
                },
                "minecraft:ageable":{
                    "duration": 1200,
                    "feed_items": ["minecraft:tnt"],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                }
            },
            "minecraft:creeper_wild": {
                "minecraft:tameable": {
                    "probability": 1,
                    "tame_items": "minecraft:tnt",
                    "tame_event": {
                        "event": "minecraft:on_tame",
                        "target": "self"
                    }
                },
                "minecraft:behavior.tempt": {
                    "priority": 5,
                    "speed_multiplier": 1.2,
                    "within_radius": 16,
                    "items": [
                        "minecraft:tnt"
                    ]
                }
            },
            "scuke_survive:is_npc": {
                "minecraft:persistent": {},
                "minecraft:sittable": {}
            }
        },
        "components": {
            "minecraft:behavior.stay_while_sitting": {
                "priority": 3
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:type_family": {
                "family": [
                    "npc"
                ]
            },
            "minecraft:behavior.follow_owner": {
                "priority": 4,
                "speed_multiplier": 1.2,
                "start_distance": 8,
                "stop_distance": 2,
                "can_teleport": false
            },
            "minecraft:breathable": {
                "total_supply": 15,
                "suffocate_time": 0
            },
            "minecraft:nameable": {
                "always_show": true
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.8
            },
            "minecraft:movement": {
                "value": 0.2
            },
            "minecraft:navigation.walk": {
                "can_path_over_water": true
            },
            "minecraft:movement.basic": {},
            "minecraft:jump.static": {},
            "minecraft:can_climb": {},
            "minecraft:health": {
                "value": 20,
                "max": 20
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:damage_sensor": {
                "triggers": {
                    "on_damage": {
                        "filters": {
                            "test": "has_damage",
                            "value": "fatal"
                        },
                        "event": "minecraft:start_exploding"
                    },
                    "deals_damage": false
                }
            },
            "minecraft:interact": {
                "interactions": {
                    "on_interact": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_equipment",
                                    "domain": "hand",
                                    "subject": "other",
                                    "value": "minecraft:tnt",
                                    "operator": "not"
                                }
                            ]
                        }
                    },
                    "hurt_item": 0,
                    "swing": true,
                    "play_sounds": "ignite",
                    "interact_text": "action.interact.scuke_survive_npc"
                }
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:behavior.float": {
                "priority": 0
            },
            "minecraft:behavior.avoid_mob_type": {
                "priority": 3,
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "ocelot"
                                }
                            ]
                        },
                        "max_dist": 6,
                        "sprint_speed_multiplier": 1.2
                    }
                ]
            },
            "minecraft:behavior.random_stroll": {
                "priority": 5,
                "speed_multiplier": 1
            },
            "minecraft:behavior.look_at_player": {
                "priority": 6,
                "look_distance": 8
            },
            "minecraft:behavior.random_look_around": {
                "priority": 6
            },
            "minecraft:behavior.hurt_by_target": {
                "priority": 2
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:on_target_escape": {
                "event": "minecraft:stop_exploding",
                "target": "self"
            },
            "minecraft:conditional_bandwidth_optimization": {}
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "minecraft:creeper_wild"
                    ]
                }
            },
            "minecraft:start_exploding_forced": {
                "add": {
                    "component_groups": [
                        "minecraft:forced_exploding"
                    ]
                }
            },
            "minecraft:start_exploding": {
                "add": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            },
            "minecraft:stop_exploding": {
                "remove": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            },
            "minecraft:on_tame": {
                "add": {
                    "component_groups": [
                        "minecraft:creeper_tamed"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:creeper_wild"
                    ]
                }
            },
            "scuke_survive:on_npc_spawn": {
                "add": {
                    "component_groups": [
                        "scuke_survive:is_npc"
                    ]
                }
            }
        }
    }
}