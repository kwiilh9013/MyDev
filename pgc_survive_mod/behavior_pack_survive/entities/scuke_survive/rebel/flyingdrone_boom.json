            "minecraft:follow_range": {
                "value": 64,
                "max": 64
            },
            "minecraft:behavior.circle_around_anchor": {
                "priority": 4
            }
        },
        "events": {
            "minecraft:start_exploding": {
                "add": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            },
            "minecraft:stop_exploding": {
                "remove": {
                    "component_groups": [
                        "minecraft:exploding"
                    ]
                }
            }
        }
    }
}