            "identifier": "scuke_survive:flyingdrone_boom",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true
        },
        "component_groups": {
            "minecraft:exploding": {
                "minecraft:explode": {
                    "fuse_length": 0.3,
                    "fuse_lit": true,
                    "power": 3,
                    "causes_fire": false,
                    "destroy_affected_by_griefing": true
                }
            }
        },
        "components": {
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:health": {
                "value": 20
            },
            "minecraft:fire_immune": {},
            "minecraft:movement": {
                "value": 2.0
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:experience_reward": {
                "on_death": "query.last_hit_by_player ? 5 + (query.equipment_count * Math.Random(1,3)) : 0"
            },
            "minecraft:physics": {
                "has_gravity": false
            },
            "minecraft:collision_box": {
                "width": 0.5,
                "height": 0.5
            },
            "minecraft:nameable": {},
            "minecraft:breathable": {
                "total_supply": 150000,
                "generates_bubbles": false,
                "breathes_water": true,
                "breathes_solids": true,
                "suffocate_time": 0
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:type_family": {
                "family": [
                    "survive",
                    "rebel",
                    "mob"
                ]
            },
            "minecraft:behavior.nearest_prioritized_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "has_tag",
                                    "subject": "other",
                                    "value": "task_entity"
                                }
                            ]
                        },
                        "max_dist": 64,
                        "priority": 0
                    },
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "player"
                                },
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "mercenary"
                                }
                            ]
                        },
                        "max_dist": 35,
                        "priority": 1
                    },
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "monster"
                                }
                            ]
                        },
                        "max_dist": 35,
                        "priority": 1
                    }
                ],
                "must_see": true,
                "priority": 1,
                "reselect_targets": true
            },
            "minecraft:behavior.hurt_by_target": {
				"priority": 3,
				"entity_types": {
					"filters": [
						{
							"any_of": [
								{
									"test": "is_family",
									"operator": "!=",
									"subject": "other",
									"value": "rebel"
								}
							]
						}
					],
					"max_dist": 35
				}
			},
            "minecraft:behavior.swoop_attack": {
                "priority": 2,
                "damage_reach": 0.01,
                "speed_multiplier": 1.2,
                "delay_range": [
                    3,
                    3
                ]
            },
            "minecraft:loot": {
                "table": "loot_tables/entities/creeper.json"
            },
            "minecraft:target_nearby_sensor": {
                "inside_range": 1,
                "outside_range": 3,
                "on_inside_range": {
                    "event": "minecraft:start_exploding",
                    "target": "self"
                },
                "on_outside_range": {
                    "event": "minecraft:stop_exploding",
                    "target": "self"
                },
                "on_vision_lost_inside_range": {
                    "event": "minecraft:stop_exploding",
                    "target": "self"
                }
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
